{
    "name": "fnbot-client",
    "version": "1.2.1",
    "serverVersion": "v1.2",
    "description": "Fortnite lobby bot that is able to equip all skins, emotes and backpacks.",
    "main": "index.js",
    "dependencies": {
        "fnbr": "^1.3.1",
        "i18next": "^17.3.1",
        "i18next-node-fs-backend": "^2.1.3",
        "inquirer": "^7.3.3",
        "json-beautify": "^1.1.1",
        "node-fetch": "^3.1.1",
        "opn": "^6.0.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "pkg": "^4.4.9"
    },
    "scripts": {
        "run": "node index.js",
        "setup": "node index.js -r",
